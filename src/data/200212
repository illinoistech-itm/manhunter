Content-Type: application/octet-stream;
	name="Categories.java"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="Categories.java"

package npg1.search;

import java.util.Arrays;
import java.util.StringTokenizer;
import org.apache.lucene.index.IndexReader;
import java.util.HashSet;
import java.util.Iterator;
import java.util.Vector;

public class Categories {

	public Vector getCategories(String index_dir) {

		try {
			IndexReader reader =3D IndexReader.open(index_dir);
=09
			int num_docs =3D reader.numDocs();

			HashSet categories =3D new HashSet();

			String codes =3D null;
=09
			for (int i =3D 0 ; i < num_docs; i++ ) {
	=09
				codes =3D reader.document(i).get("code");

				if(codes !=3D null && codes.length() > 0) {
=09
					StringTokenizer codelist =3D new StringTokenizer(codes);
=09
					while(codelist.hasMoreTokens()) {
=09
						categories.add((String)codelist.nextToken());
					}
				}
			}
			Object[] cat_array =3D categories.toArray();
			Arrays.sort(cat_array);
			return new Vector(Arrays.asList(cat_array));
		} catch (Exception e) {
			return null;
		}
	}

	public static void main(String[] args) {

		//arg[0] is the index directory, relative or absolute
		Categories cats =3D new Categories();
		Vector categories =3D cats.getCategories(args[0]);
		System.out.println("Found the following " + categories.size() + " =
unique categories");

		for(int i =3D 0; i < categories.size(); i++) {
			System.out.println(categories.get(i));
		}
	}

}

	

