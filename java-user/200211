> =3D=3D
> package com.tropo.lucene;
>=20
> import java.io.*;
> import java.util.*;
>=20
> import org.apache.lucene.analysis.*;
> import org.apache.lucene.analysis.standard.*;
> import org.apache.lucene.search.*;
> import org.apache.lucene.queryParser.*;
>=20
> public class KeywordProblem
> {
> 	/**
> 	 *
> 	 */
> 	public static void main(String[] args)
> 		throws Throwable
> 	{
> 		String body =3D "body";
> 		String url =3D "url";
>=20
> 		String[] lines =3D new String[] {
> 			"foo",
> 			"body:foo",
> 			"url:http://www.tropo.com/",
> 			"url:\"http://www.tropo.com/\""
> 		};
>=20
> 		Analyzer a =3D new StandardAnalyzer();
> 		for ( int i =3D 0; i < lines.length; i++)
> 		{
> 			Query query =3D QueryParser.parse( lines[i], url,
> a);
> 			o.println( "From: " + lines[i]);
> 			o.println( "To  : " + query.toString( url));
> 			o.println();
> 		}
> 	}
> 	private static PrintStream o =3D System.out;
> }
